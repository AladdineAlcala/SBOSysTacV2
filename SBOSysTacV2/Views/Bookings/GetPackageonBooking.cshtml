@using SBOSysTacV2.ViewModel
@model TransactionDetailsViewModel


<div class="row">
    <section class="content">

        <div class="row">
            <div class="col-md-6" id="bookmenus" style="height:100%;">

                @{ Html.RenderAction(Model.Package_Trans.p_type.TrimEnd() == "sd" ? "GetSnacksPartial" : "GetBookMenusPartial", "Bookings", new { transactionId = Model.transactionId }); }

            </div>

            <div class="col-md-6">
                <div class="row">
                    <div class="col-md-12" id="addons">

                        @{ Html.RenderAction("GetListofAddons", "Bookings", new { transid = Model.transactionId });}
                    </div>


                </div>

                <div class="row">
                    <div class="col-md-12 my-5" id="addonsdetails">

                    </div>
                </div>

            </div>
        </div>

    </section>
</div>



